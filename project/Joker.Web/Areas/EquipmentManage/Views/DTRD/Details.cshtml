@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = $.request("keyValue");
    $(function () {

        $.ajax({
            url: "/EquipmentManage/DTRD/GetFormJson",
            data: { keyValue: keyValue },
            dataType: "json",
            async: false,
            success: function (data) {
                $("#form1").formSerialize(data);
                $("#form1").find('.form-control,select,input').attr('readonly', 'readonly');
            }
        });
    });

</script>
<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <ul class="nav nav-tabs">
            <li class="active"><a href="">参数信息</a></li>
            @*<li class="active"><a href="#">参数信息</a></li>*@
            @*<li ><a href="#">参数信息</a></li>*@
        </ul>
        <div style="padding-top: 20px; margin-right: 30px;">
            <table class="form">
                <tr>
                    <th class="formTitle">装置类型编号</th>
                    <td class="formValue">
                        <input id="F_Data1" name="F_Data1" type="text" class="form-control required" placeholder="请输入装置类型编号" />
                    </td>
                    <th class="formTitle">装置描述</th>
                    <td class="formValue">
                        <input id="F_Data2" name="F_Data2" type="text" class="form-control required" placeholder="请输入装置描述" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">分段定义</th>
                    <td class="formValue">
                        <input id="F_Data3" name="F_Data3" type="text" class="form-control required" placeholder="请输入分段定义" />
                    </td>
                    <th class="formTitle">寄存器地址</th>
                    <td class="formValue">
                        <input id="F_Data4" name="F_Data4" type="text" class="form-control required" placeholder="请输入寄存器地址" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">寄存器占用数量</th>
                    <td class="formValue">
                        <input id="F_Data5" name="F_Data5" type="text" class="form-control required" placeholder="请输入寄存器占用数量" />
                    </td>
                    <th class="formTitle">寄存器类型</th>
                    <td class="formValue">
                        <input id="F_Data6" name="F_Data6" type="text" class="form-control required" placeholder="请输入寄存器类型" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">寄存器描述</th>
                    <td class="formValue">
                        <input id="F_Data7" name="F_Data7" type="text" class="form-control required" placeholder="请输入寄存器描述" />
                    </td>
                    <th class="formTitle">数据格式</th>
                    <td class="formValue">
                        <input id="F_Data8" name="F_Data8" type="text" class="form-control required" placeholder="请输入数据格式" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">单位/范围</th>
                    <td class="formValue">
                        <input id="F_Data9" name="F_Data9" type="text" class="form-control required" placeholder="请输入单位/范围" />
                    </td>
                    <th class="formTitle">备注</th>
                    <td class="formValue">
                        <input id="F_Data10" name="F_Data10" type="text" class="form-control required" placeholder="请输入备注" />
                    </td>
                </tr>  
            </table>
        </div>
    </div>
</form>
